{
  "manifest_version": 2,
  "name": "Search Replacer",
  "version": "0.1",
  "description": "An extension to spoof a search result given a specific keyword",
  "content_scripts": [
    {
      "matches": [
        "*://www.google.com/*",
        "*://www.google.co.uk/*"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": [ "background.js" ]
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs"
  ],
  "web_accessible_resources": [
    "generate-google-results.js"
  ],
  "options_page": "options.html"
}
